{
  "name": "betah-configs",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:eslint-config": "turbo run build --filter=@betahcoding/eslint-config",
    "build:ts-config": "turbo run build --filter=@betahcoding/ts-config",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=18"
  }
}
